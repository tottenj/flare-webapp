rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    
   match /org/proofs/{uid}/{allPaths=**} {
      allow write: if request.auth != null && request.auth.uid == uid;
    }

   match /users/{userId}/{fileName} {
      allow write: if request.auth != null
                   && request.auth.uid == userId
      allow read: if true;
    }
  }
}
