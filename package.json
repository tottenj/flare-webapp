{
  "name": "flare-webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:dev": "npx dotenv -e .env -e .env.dev -- next dev --turbopack",
    "dev:cyp": "npx dotenv -e .env -e .env.test -- next dev",
    "test:watch": "jest --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "vitest --project=storybook",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write .",
    "chromatic": "npx chromatic --project-token=chpt_63b29bbeaa870ab",
    "emulators:start": "firebase emulators:start --only auth,functions,storage",
    "db:reset": "prisma migrate reset --force --skip-seed",
    "seed:test": "node prisma/seedTest.js",
    "e2e:setup": "npx wait-on tcp:5432 && npx dotenv -e .env -e .env.test -- npx prisma migrate reset --force --skip-seed",

    "cypress:open": "wait-on http://localhost:3000 && cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "npm run e2e:setup && concurrently -k -n 'EMULATORS,APP,CYPRESS' -c 'blue.bold,green.bold,magenta.bold' \"npm run emulators:start\" \"npm run dev:cyp\" \"npm run wait-and-test\"",
    "test:e2e:local": "npm run e2e:setup && concurrently -k -n 'EMULATORS,APP,CYPRESS' -c 'blue.bold,green.bold,magenta.bold' \"npm run emulators:start\" \"npm run dev:cyp\" \"npm run wait-and-open\"",
    "wait-and-test": "node scripts/wait-for-emulators.js && npx wait-on http-get://localhost:3000/api/health && npm run cypress:run",
    "wait-and-open": "node scripts/wait-for-emulators.js && npx wait-on http-get://localhost:3000/api/health && npm run cypress:open",
    "dev:migrate": "npx dotenv -e .env.dev -- prisma migrate dev",
    "dev:reset": "npx dotenv -e .env.dev -- prisma migrate reset",
    "dev:studio": "npx dotenv -e .env.dev -- prisma studio",
    "test": "jest",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "npm run e2e:setup && firebase emulators:exec --only auth,firestore,functions 'jest --selectProjects integration'",
    "coverage": "jest --coverage --reporters=default --reporters=jest-junit"
  },
  "type": "module",
  "jest-junit": {
    "outputDirectory": "./reports/jest",
    "outputName": "junit.xml"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
